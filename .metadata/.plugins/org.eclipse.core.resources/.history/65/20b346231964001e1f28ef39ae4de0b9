	enum FileType{
		
		PDFFILE,INPUTSTRAM,BYTEARRAY
		
	}
	
	enum ScanMode{
		
		QRCODE,Barcode
		
		
	}
	
	enum DocType{
		
		E_INVOICE,GRN
		
	}
	
	enum ScanData{
		
		RAW,DECODED_DATA
		
	}
	
	
	
	private boolean signValidata;

}
